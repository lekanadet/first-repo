            </tr>
        {{/each}}
 
  </table>

<table class="table">

  {{#each data2}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.price}}</td>
                <td><img src ={{this.picture}}></td>
                <td>{{this.town}}</td>
                <td>{{this.street}}</td>
                <td>{{this.bed}}</td>
                <td>{{this.bath}}</td>
