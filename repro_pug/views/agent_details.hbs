</head>

<div class="container">
  <h1>AGENT PAGE</h1>
  <h2></h2>
  
  <br /><br />
   <br /><br />
   <table class="table">
  
       {{#each data}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.repro_no}}</td>
                <td><img src ={{this.picture}}></td>
                <td>{{this.role}}</td>
                <td>{{this.email}}</td>
