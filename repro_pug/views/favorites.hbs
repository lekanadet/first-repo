<head>
    <link rel="stylesheet" href="/css/styles.css">
    <a href="/" class="btn btn-success">Home</a>
</head>

<div class="container">
  <h1>FAVORITES</h1>
  <h2></h2>
  
  <br /><br />
   <br /><br />
   <table class="table">
  
       {{#each data}}
            <tr>
                <td>{{this.property_type}}</td>
                <td>{{this.price}}</td>
                <td><img src ={{this.picture}}></td>
                <td>{{this.town}}</td>
                <td>{{this.street}}</td>
                <td>{{this.bed}}</td>
                <td>{{this.bath}}</td>
                <td> <a href="/unfavorited/{{this.property_id}}" class="btn btn-info btn-sm">Unlike</a></td>
            </tr>
        {{/each}}
 
  </table>

  
</div>