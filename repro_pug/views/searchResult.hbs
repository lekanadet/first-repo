<head>
    <link rel="stylesheet" href="/css/styles.css">
    <a href="/" class="btn btn-success">Home</a>
    {{#each data3}}
    <a href="/save-search/{{this.max}}" class="btn btn-info btn-sm">Save</a>
        {{/each}}
</head>
<div class="container">
  <h1>SEARCHPAGE</h1>
  <h2></h2>
  <br /><br />
  <table class="table">
  
       {{#each data3}}
            <tr>
                <td>Max Search Id is {{this.max}} </td>
            </tr>
        {{/each}}
 
  </table>
   <br /><br />

<table class="table">
  
       {{#each data2}}
            <tr>
                <td>Your search Returned {{this.count}} results</td>
            </tr>
        {{/each}}
 
  </table>
 <br /><br />
   <br /><br />

  <table class="table">
  
       {{#each data}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.name}}</td>
                <td>{{this.price}}</td>
                <td>Picture Count - {{this.picture_count}}</td>
                <td><a href="/view-property/{{this.id}}" class="btn btn-danger btn-sm"><img src ={{this.picture}}></a></td>
                <td>{{this.town}}</td>
                <td>{{this.street}}</td>
                <td>{{this.bed}}</td>
                <td>{{this.bath}}</td>
                 <td> <a href="/favorited/{{this.id}}" class="btn btn-info btn-sm">like</a></td>
                 <td> <a href="/view-property/{{this.id}}" class="btn btn-info btn-sm">view</a></td>
            </tr>
        {{/each}}
 
  </table>

</div>